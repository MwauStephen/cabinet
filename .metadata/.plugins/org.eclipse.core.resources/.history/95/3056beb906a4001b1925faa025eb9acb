package ArrayDynamic;

import java.util.ArrayList;

public class ArrayDynamic {

	public static void main(String[] args) {
		ArrayList<Integer>list=new ArrayList<>();
		list.add(20);
		list.add(30);
		list.add(40);
		list.add(50);
		list.remove(0);
		list.indexOf(30);
		list.lastIndexOf(20);
		list.contains(30);
		list.size();
		list.toArray();
		System.out.println(list);

	}

}
